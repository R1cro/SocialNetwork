$('.microposts').append('<%= j render(@liked_posts) %>');
$("div[id|='reply-to-micropost']").hide()
<% if @liked_posts.next_page %>
  $('.pagination').replaceWith('<%= j will_paginate(@liked_posts) %>');
  $('#loading').hide();
<% else %>
  $('.pagination').remove();
  $('#loading').remove();
<% end %>
<% sleep 1 %>
