<% if @feed_items.any? %>
  <div class="container">
    <h5>Last users posts</h5>
    </div>
  <ol class="microposts">
    <%= render @feed_items %>
  </ol>
  <% if logged_in? %>
    <div id="scrolling">
      <div id="loading"><img src="/ajax-loader.gif" alt="Loading..."></div>
      <%= will_paginate @feed_items %>
    </div>
  <% end %>
<% end %>
